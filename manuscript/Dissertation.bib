	keywords = {efficiency, household bargaining, Labor Economics, limited committment, marriage, Microeconomics, migration, Population Economics, Social Issues},
	pages = {153--167},
	file = {2003_Lundberg_Pollak_Efficiency in Marriage_Review of Economics of the Household.pdf:C\:\\Users\\sinan\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1c8xr76o.default\\zotero\\storage\\RW4DCIQE\\2003_Lundberg_Pollak_Efficiency in Marriage_Review of Economics of the Household.pdf:application/pdf;Snapshot:C\:\\Users\\sinan\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1c8xr76o.default\\zotero\\storage\\Q3MFMQMD\\A1025041316091.html:text/html}
