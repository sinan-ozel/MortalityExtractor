	note = {00486},
	keywords = {\_tablet, Multilevel models, Neighborhood effects, Social capital, United States},
	pages = {1797--1805},
	file = {2003_Lochner et al_Social capital and neighborhood mortality rates in Chicago_Social Science & Medicine.pdf:C\:\\Users\\sinan\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1c8xr76o.default\\zotero\\storage\\FN62SCP3\\2003_Lochner et al_Social capital and neighborhood mortality rates in Chicago_Social Science & Medicine.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sinan\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\1c8xr76o.default\\zotero\\storage\\49WEZZMU\\S0277953602001776.html:text/html}
